#include <bits/stdc++.h>
using namespace std;

void fun( int num,int a[],int n)
{
    int cnt =0;
    for(int j=0;j<n;j++)
        {
            if(a[j]==num)
            cnt++;
        }
        cout<<num<<"--->"<<cnt<<endl;
    
}

int main()
{
    int a[]={1,2,3,4,3,5,6,1,10,2};
    int n = sizeof(a) / sizeof(a[0]);
    fun(1,a,n);
    fun(2,a,n);
    fun(10,a,n);
    
   
    
       

    return 0;
}
